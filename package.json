{
	"name": "my-store-app",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"deploy:s3": "serverless client deploy --no-config-change --no-policy-change --no-cors-change",
		"deploy": "cdk deploy",
		"preview": "npm run build && vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"lint": "eslint --ignore-path .gitignore --fix src",
		"prettier": "prettier src --write",
		"cdk:bootstrap": "npx cdk bootstrap",
		"cloudfront:setup": "cdk deploy --profile node-aws-course"
	},
	"dependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.18",
		"@mui/material": "^5.14.18",
		"@types/yup": "^0.29.14",
		"aws-cdk-lib": "^2.110.0",
		"awscdk-v1-stack-finder": "^1.0.120",
		"axios": "^1.6.2",
		"formik": "^2.2.9",
		"msw": "^2.0.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-query": "^3.39.1",
		"react-router-dom": "^6.19.0",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.2.1",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"@typescript-eslint/parser": "^5.30.5",
		"@vitejs/plugin-react": "^1.3.2",
		"@vitest/ui": "^0.18.0",
		"aws-cdk": "^1.94.1",
		"c8": "^7.11.3",
		"dotenv": "^16.3.1",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.30.1",
		"jsdom": "^20.0.0",
		"prettier": "2.7.1",
		"typescript": "^4.7.4",
		"vite": "^2.9.13",
		"vitest": "^0.17.1"
	},
	"msw": {
		"workerDirectory": "public"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"overrides": {
		"graphql": "^17.0.0-alpha.2"
	}
}
